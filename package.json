    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.0",
    "react-zoom-pan-pinch": "^3.4.4",
    "skinview3d": "^3.0.1",
    "socket.io-client": "^4.7.5",
    "styled-components": "^6.1.8",
    "yup": "^1.4.0"
